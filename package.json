{
	"name": "jewels-server",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite dev --port 3000 --host 0.0.0.0",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"check-eol": "tsx src/console.ts"
	},
	"dependencies": {
		"express": "4.19.2",
		"nodemailer": "6.9.13",
		"tsx": "4.15.5"
	},
	"devDependencies": {
		"@auth/sveltekit": "1.2.0",
		"@fontsource/albert-sans": "5.0.13",
		"@fontsource/oxygen-mono": "5.0.20",
		"@fontsource/urbanist": "5.0.20",
		"@jinyacms/cosmo": "1.0.2",
		"@svelte-put/qr": "1.2.1",
		"@sveltejs/adapter-node": "5.1.0",
		"@sveltejs/kit": "2.5.15",
		"@sveltejs/vite-plugin-svelte": "3.1.1",
		"@types/eslint": "8.56.10",
		"@types/express": "4.17.21",
		"@types/nodemailer": "6.4.15",
		"@typescript-eslint/eslint-plugin": "7.13.0",
		"@typescript-eslint/parser": "7.13.0",
		"eslint": "8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.39.4",
		"mongodb": "6.7.0",
		"prettier": "3.3.2",
		"prettier-plugin-svelte": "3.2.4",
		"sass": "1.77.5",
		"svelte": "4.2.18",
		"svelte-check": "3.8.0",
		"tslib": "2.6.3",
		"typescript": "5.4.5",
		"vite": "5.3.1"
	},
	"type": "module"
}
