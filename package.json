{
	"name": "jewels-server",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"dev": "vite dev --port 3000 --host 0.0.0.0",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"check-eol": "tsx src/console.ts"
	},
	"dependencies": {
		"express": "4.21.1",
		"nodemailer": "6.9.15",
		"tsx": "4.19.1"
	},
	"devDependencies": {
		"@auth/sveltekit": "1.7.1",
		"@eslint/eslintrc": "3.1.0",
		"@eslint/js": "9.13.0",
		"@fontsource/albert-sans": "5.1.0",
		"@fontsource/oxygen-mono": "5.1.0",
		"@fontsource/urbanist": "5.1.0",
		"@jinyacms/cosmo": "1.0.3",
		"@svelte-put/qr": "1.2.1",
		"@sveltejs/adapter-node": "5.2.8",
		"@sveltejs/kit": "2.7.1",
		"@sveltejs/vite-plugin-svelte": "3.1.2",
		"@types/express": "5.0.0",
		"@types/nodemailer": "6.4.16",
		"@typescript-eslint/eslint-plugin": "8.10.0",
		"@typescript-eslint/parser": "8.10.0",
		"eslint": "9.13.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.45.1",
		"globals": "15.11.0",
		"mongodb": "6.9.0",
		"prettier": "3.3.3",
		"prettier-plugin-svelte": "3.2.7",
		"sass": "1.80.2",
		"svelte": "4.2.19",
		"svelte-check": "4.0.5",
		"svelte-eslint-parser": "0.42.0",
		"tslib": "2.8.0",
		"typescript": "5.6.3",
		"typescript-eslint": "8.10.0",
		"vite": "5.4.9"
	},
	"type": "module"
}
